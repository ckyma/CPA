    #include <iostream>
    #include <string>
    using namespace std;
    
    int main() {
        string s1 = "aleph";
        string s2 = "alpha";
        string s;
        s1.swap(s2);
        s2.swap(s);
        s.swap(s2);
        cout << s2;
        return 0;
    }
    
 

